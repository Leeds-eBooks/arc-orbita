// @flow

type Video = $ReadOnly<{|
  type: 'video',
  name: string,
  src: string,
  thumb: string,
  backgroundColor: string,
  mainColor: string,
  info: {
    title: string,
    description: string,
  },
|}>

type Image = $ReadOnly<{|
  type: 'image',
  name: string,
  src: string,
  backgroundColor: string,
  info: {
    title: string,
    description: string,
  },
|}>

type ContentUnion = Video | Image

declare module.exports: $ReadOnlyArray<ContentUnion>
